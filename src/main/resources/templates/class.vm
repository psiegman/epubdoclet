<html>
<head>
	<!-- ${clazz.aboutMessage} -->
	<title>${clazz.name}</title>
	<link rel="stylesheet" href="../../css/reset.css" type="text/css"/>
	<link rel="stylesheet" href="../../css/style.css" type="text/css"/>
</head>
<body>
<b>${clazz.package}</b>
<h1>${clazz.name}</h1>
<p>
${clazz.commentText}
</p>
<table>
<tr>
<td colspan="2" class="tableheader">Method summary</td>
</tr>
#foreach( $method in $clazz.methods )
<tr>
	<td>${method.modifiers} ${method.returnType.typeName()}</td>
	<td>
		<span class="method-name">${method.name}</span>(#foreach($parameter in $method.parameters())#if($velocityCount > 1), #end${parameter.typeName()} ${parameter.name()}#end)<br/>
		<span class="first-line">${method.commentFirstLine}</span>
	</td>
</tr>
#end
</table>
</body>
</html>